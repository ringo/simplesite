{
  "server": {
    "vhosts": ["127.0.0.1", "localhost"],
    "http": {
      "host": "127.0.0.1",
      "port": 8080
    }
  },
  "content": "./content",
  "templates": "./templates",
  "static": [
    {
      "path": "./static",
      "index": "index.html",
      "baseURI": "/static",
      "options": {
        "servePrecompressed": true,
        "maxAge": 0,
        "lastModified": true,
        "dotfiles": "ignore"
      }
    }
  ],
  "welcomePage": "index.md"
}